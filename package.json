{
	"name": "smartbanner-ts",
	"version": "1.4.0",
	"description": "Smartbanner library written using TS",
	"author": "David My≈°ka",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/miskith/smartbanner-ts/issues"
	},
	"homepage": "https://github.com/miskith/smartbanner-ts#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/miskith/smartbanner-ts.git"
	},
	"type": "module",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"types": "dist/esm/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"scripts": {
		"build": "pnpm clean && pnpm build:esm && pnpm build:cjs && pnpm build:css",
		"build:esm": "tsc -p tsconfig.esm.json",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:scss": "sass src/scss/smartbanner.scss dist/css/smartbanner.min.css --no-source-map",
		"build:css": "pnpm build:scss && postcss dist/css/smartbanner.min.css --no-map --use autoprefixer cssnano -o dist/css/smartbanner.min.css",
		"format": "pretty-quick",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"js-cookie": "^3.0.5"
	},
	"devDependencies": {
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^25.0.2",
		"autoprefixer": "^10.4.22",
		"cssnano": "^7.1.2",
		"postcss": "^8.5.6",
		"postcss-cli": "^11.0.1",
		"prettier": "^3.6.2",
		"pretty-quick": "^4.2.2",
		"sass": "^1.94.2",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3"
	}
}
